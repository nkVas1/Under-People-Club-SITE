{
  "projects": [
    {
      "src": "frontend",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next/standalone"
      }
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/.next/standalone",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.underpeople.club/api/:path*"
    }
  ]
}
